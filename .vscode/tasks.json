{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Build Gurotopia",
            "type": "shell",
            "command": "C:/msys64/usr/bin/bash.exe",
            "args": [
                "-c",
                "export PATH=\"$PATH:/c/msys64/ucrt64/bin:/c/msys64/usr/bin\" && make -j4"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "Clean Gurotopia",
            "type": "shell",
            "command": "C:/msys64/usr/bin/bash.exe",
            "args": [
                "-c",
                "export PATH=\"$PATH:/c/msys64/ucrt64/bin:/c/msys64/usr/bin\" && make clean"
            ]
        }
    ]
}